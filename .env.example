# Example .env file.

# Directory for the database file to be saved in. Leave empty to save in the root of disgotify.
DATABASE_DIR: /var/disgotify

# Your bot's authentication token.
DISCORD_TOKEN: aAbBcCdDeEfFgGhHiIjJkKlLmMnNoOpPqQrRsStTuUvVwWxXyYzZ.0-9.-_

# Your own user ID in Discord.
DEVELOPER_ID: 00000000000000000

# Global prefix for all commands (at least one character).
COMMAND_PREFIX: +

# Interval in which the database will be checked for due reminders *IN MILLISECONDS*.
REMINDER_INTERVAL: 10000

# Whether the log level should be on DebugLevel.
DEBUG: false